<div class="modal-content">
<div class="col-lg-12">
<h1 class="page-header" style="color:#337ab7;">REGISTRAR EGRESADO</h1>
</div>
<br/>
<br/>
<div id="dataTables-example_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
<div class="panel-body">
<%= simple_form_for @egresado do |f|%>
<div class="container"
<div class="col-md-8 col-md-offset-2">
<div class="card" style="color:#337ab7;">
<div class="card-body d-flex justify-content-between align-items-center">
    <%= f.input :nombre,
     placeholder:"Digite Nombre"%>
&nbsp
&nbsp
&nbsp
&nbsp
    <%= f.input :apellidos,
    placeholder:"Digite Apellidos"%>
&nbsp
<%= f.input :correo,
 placeholder:"Digite Correo"%>

  <p><br>
  <%= f.input :ingreso%>
  &nbsp
  &nbsp
  &nbsp
  &nbsp
  <%= f.input :egreso%>
  </p>

<p><br>
<%= f.input :contingente, placeholder:"Digite Contingente"%>
&nbsp

<%= f.input :direccion,
 placeholder:"Digite Direccion"%>
&nbsp
 <%= f.input :telefono,
  placeholder:"Digite Telefono"%>
</p>
<p><br>
  <%= f.association  :facultad, label_method: :nombre%>
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
&nbsp
  <%= f.association :programa, label_method: :nombre%>
&nbsp
&nbsp
&nbsp
&nbsp
<%= f.submit 'Guardar', method: :index, data: { confirm: 'Guardar Egresado' },
    class:"btn btn-primary active"%>
</p>
</div>
</div>
</div>
</div>
<%end%>
</div>
</div>
</div>
